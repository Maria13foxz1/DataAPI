@model Messanger.Models.NameInfoModel

@{
    Layout = null;
    ViewBag.Title = "Відповідь";
}

<!DOCTYPE html>
<html>
<head class="font">
    <title> Відповідь:</title>
                <p class="basicsymbols">Ім'я: <span class="bluesymbols">@Model.Name</span></p>
                <p class="basicsymbols">Рік: <span class="bluesymbols">@Model.Age</span></p> 
                <p class="basicsymbols">Стать: <span class="bluesymbols">@Model.Gender</span></p> 
                <p class="basicsymbols">Ймовірність даної статті: <span class="bluesymbols"> @Model.GenderProbability </span></p> 
                <p class="basicsymbols">Країна: <span class="bluesymbols"></span>@Model.NationalityData</p>
                @{
                    var country =  @Model.NationalityData;
                    foreach (var item in country)
                    {
                        Console.WriteLine($"Країна: {item}, Ймовірність: {item}");
                    }
                }
</head>
<body class="font">

</body>
</html>
<style>
.font {
         position: absolute;
         left: 0;
         top: 0;
         width: 100%;
         height: 100%;
         font-family: "Roboto", sans-serif;
         background: linear-gradient(#012626, #091e1e, #100f0f, #02181c, #012626);
         overflow: hidden;
         margin-top: 350px;
       }</style>